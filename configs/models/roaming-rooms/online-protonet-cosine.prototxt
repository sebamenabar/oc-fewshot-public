backbone_class: "resnet_12_backbone"
model_class: "online_proto_sigmoid_net"
memory_class: "ssl_min_dist_proto_memory"
num_classes: 10
fix_unknown: true
roi_pooling_backbone: true
ssl_store_schedule: true
num_episodes: -1

resnet_config: {
    height: 160
    width: 120
    num_channels: 3
    num_residual_units: 1
    num_residual_units: 1
    num_residual_units: 1
    num_residual_units: 1
    num_filters: 32
    num_filters: 64
    num_filters: 128
    num_filters: 256
    strides: 2
    strides: 2
    strides: 2
    strides: 1
    weight_decay: 2e-4
    normalization: "batch_norm"
    global_avg_pool: false
    data_format: "NCHW"
    add_last_relu: false
}

optimizer_config: {
    optimizer: "adam"
    lr_list: 1e-3
    lr_list: 1e-4
    lr_list: 1e-5
    lr_decay_steps: 8000
    lr_decay_steps: 16000
    max_train_steps: 20000
    batch_size: 2
    lr_scaling: false
}

memory_net_config: {
    max_classes: 20
    radius_init: 10.0
    radius_init_write: 10.0
    similarity: "cosine"
}

train_config: {
    steps_per_val: 500
    steps_per_save: 500
    steps_per_log: 10
}
